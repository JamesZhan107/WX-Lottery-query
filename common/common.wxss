.weui-loading {
    margin: 0 10rpx;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    vertical-align: middle;
    animation: weuiLoading 1s steps(12,end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}

.weui-loadmore {
    width: 100%;
    height: 80rpx;
    margin: 20rpx auto;
    line-height: 1.6em;
    font-size: 11pt;
    text-align: center;
}

.weui-refer {
    width: 100%;
    margin: 20rpx auto;
    line-height: 1.6em;
    font-size: 11pt;
    text-align: center;
}

.weui-loadmore__tips {
    height: 40rpx;
    display: inline-block;
    vertical-align: middle;
}

.wux-prompt__empty {
    display: flex;
    align-items: center;
    height: 934rpx;
    -webkit-box-align: center;
    justify-content: center;
    color: #939393;
}

.wux-prompt__bd {
    align-items: center;
    margin-top: 0;
    text-align: center;
}

.wux-prompt__icon {
    width: 200rpx;
    height: 200rpx;
}

.wux-prompt__title {
    font-size: 32rpx;
    font-weight: bold;
    color: #333;
    margin: 4rpx 0;
}

.wux-prompt__text {
    font-size: 11pt;
    margin: 4rpx 0;
}